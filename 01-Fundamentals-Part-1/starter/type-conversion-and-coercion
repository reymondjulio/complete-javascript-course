// Type Conversion

const inputYear = "1997";
console.log(Number(inputYear), inputYear);
console.log(Number(inputYear) + 18);

console.log(Number(`Reymond`));
console.log(typeof NaN);

console.log(String(23), 23);

// Type Coercion

console.log("I am" + 26 + " years old");
console.log("23" - "10" - 3);
console.log("23" / "2");

let n = "1" + 1;
n = n - 1;
console.log(n);
